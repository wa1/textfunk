{
    "name": "Wordfunk",
    "version": "0.1",
    "description": "Get synonyms for a word, in context menu.",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]      
    },
    "content_scripts": [ {
       "all_frames": true,
       "js": [ "content.js" ],
       "match_about_blank": true,
       "matches": [ "<all_urls>" ]
    } ],
    "permissions": [
      "contextMenus", "tabs"
    ]
  }